alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias ga='git add .'
alias gap='git add -p'
alias gai='git add -i'
alias gc='git commit -m'
alias gcc='git checkout'
alias gp='git push'
alias gs='git status'
alias gl='git log'
alias gb='git branch'
alias gac='ga && gc'

alias rmall='yes | rm -r'

alias tdl='todoist list'
alias tda='todoist add'
alias tds='todoist sync'
alias tdr='rmtodoist.sh'

gacp () {
  ga
  git commit -m "$1"
  gp
}
